    "# single inheritance\n",
    "\n",
    "class A:\n",
    "    def printString(self):\n",
    "        print(\"from A\")\n",
    "        \n",
    "class B(A): # B is the child of A\n",
    "    pass\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    obj = B()\n",
    "    obj.printString()"
